import java.io.File;

public class App {

	public static void main(String[] args) {

		File F1 = new File("C:\\Users\\faiza\\Downloads\\TEST\\");
		File[] List = F1.listFiles();
		for (int i = 0; i < List.length; i++) {

	List[i].renameTo(new File("C:\\Users\\faiza\\Downloads\\TEST\\" + "Faizan" + i + ".txt"));

		}
		System.out.println("Done");
	}
}
